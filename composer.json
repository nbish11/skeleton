{
    "name": "nbish11/skeleton",
    "type": "library",
    "description": "Boilerplate code for developing a PHP application/library.",
    "keywords": [
        "nbish11",
        "skeleton"
    ],
    "homepage": "https://github.com/nbish11/skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Bishop",
            "email": "nbish11@hotmail.com",
            "homepage": "https://github.com/nbish11",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "nbish11\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "nbish11\\Skeleton\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
